{
  "name": "propdown",
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eaardal/propdown"
  },
  "scripts": {
    "start": "npm run copy-hbs && npm run babel -- -w",
    "main": "babel-node src/index.js",
    "copy-hbs": "copyfiles src/indexPage/index-template.hbs -u 2 lib/indexPage",
    "compile-docs": "babel-node src/compileDocs.js",
    "compile-index": "babel-node src/compileIndexPage.js",
    "watch-index": "nodemon -e js,hbs --exec npm run compile-index",
    "test": "npm run eslint && npm run jest",
    "test:watch": "npm run jest:watch",
    "jest:watch": "npm run jest -- --watch",
    "jest:coverage": "npm run jest -- --coverage",
    "jest:watchall": "npm run jest -- --watchAll",
    "babel": "babel src --out-dir lib --ignore *spec.js",
    "clean": "rm -rf lib/",
    "build": "npm run eslint && npm run clean && npm run copy-hbs && npm run babel --",
    "build:sourcemaps": "npm run build -- --source-maps inline",
    "eslint": "eslint src/**/*.js* test/**/*.js",
    "jest": "jest --config=jest.json",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "copyfiles": "^1.2.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "argv": "0.0.2",
    "chalk": "^2.1.0",
    "handlebars": "^4.0.10",
    "markdown-it": "^8.4.0",
    "markdown-it-emoji": "^1.4.0",
    "mkdirp": "^0.5.1",
    "react-docgen": "^2.18.0",
    "react-docgen-markdown-renderer": "^1.0.1",
    "recursive-readdir": "^2.2.1"
  }
}
